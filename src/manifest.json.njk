---
permalink: '/manifest.json'
eleventyExcludeFromCollections: true
---
{
    "short_name": "{{metadata.name}}",
    "name": "{{metadata.title}}",
    "icons": [
        {
            "src": "resources/images/favicon.ico",
            "sizes": "48x48",
            "type": "image/x-icon"
            },
            {
            "src": "resources/images/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
            },
            {
            "src": "resources/images/android-chrome-384x384.png",
            "sizes": "384x384",
            "type": "image/png"
            }
    ],
    "start_url": "{{context}}/index.html",
    "display": "standalone",
    "theme_color": "#d1dae0",
    "background_color": "#35424b"
}